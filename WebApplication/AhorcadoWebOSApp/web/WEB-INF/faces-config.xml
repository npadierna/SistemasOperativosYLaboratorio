<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd" >

    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>

    <managed-bean>
        <managed-bean-name>categoryWorkFlowBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.os.ahorcado.web.bean.category.CategoryWorkFlowBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>playerWorkFlowBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.os.ahorcado.web.bean.player.PlayerWorkFlowBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>categoryWordsWorkFlowBean</managed-bean-name>
        <managed-bean-class>
            co.edu.udea.os.ahorcado.web.bean.word.CategoryWordsWorkFlowBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
</faces-config>